<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>rest-api</name>
    <display-name>rest-api</display-name>
    <description>Exposes TeamCity API via REST-like protocol</description>
    <download-url>http://confluence.jetbrains.net/display/TW/REST+API+Plugin</download-url>
    <version>@version@</version>

    <vendor>
      <name>JetBrains, s.r.o.</name>
      <url>http://www.jetbrains.com/teamcity</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="true"/>
  <parameters>
    <parameter name="api.path">/app/rest,/app/rest/latest,/app/rest/5.0,/api</parameter>
  </parameters>
</teamcity-plugin>
