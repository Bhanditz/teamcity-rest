  </path>
  
  
  <patternset id="excluded.from.module.cctrayextension-server">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.cctrayextension-server">
    <patternset refid="excluded.from.module.cctrayextension-server"/>
  </patternset>
  
  <path id="cctrayextension-server.module.sourcepath">
    <dirset dir="${module.cctrayextension-server.basedir}/server">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.cctrayextension-server" depends="compile.module.cctrayextension-server.production,compile.module.cctrayextension-server.tests" description="Compile module cctrayExtension-server"/>
  
  <target name="compile.module.cctrayextension-server.production" description="Compile module cctrayExtension-server; production classes">
    <mkdir dir="${cctrayextension-server.output.dir}"/>
    <javac2 destdir="${cctrayextension-server.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true">
      <compilerarg line="${compiler.args.cctrayextension-server}"/>
      <bootclasspath refid="cctrayextension-server.module.bootclasspath"/>
      <classpath refid="cctrayextension-server.module.production.classpath"/>
      <src refid="cctrayextension-server.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.cctrayextension-server"/>
    </javac2>
    
    <copy todir="${cctrayextension-server.output.dir}">
      <fileset dir="${module.cctrayextension-server.basedir}/server/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.cctrayextension-server.tests" depends="compile.module.cctrayextension-server.production" description="compile module cctrayExtension-server; test classes" unless="skip.tests"/>
  
  <target name="clean.module.cctrayextension-server" description="cleanup module">
    <delete dir="${cctrayextension-server.output.dir}"/>
    <delete dir="${cctrayextension-server.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.cctrayextension-server" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.cctrayextension-server" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
